#include<stdio.h>
int main()
{
	int x,y;x=0;y=0;
	char s[20];
	scanf("%[^\n]s",s);
	int z=strlen(s);
	if(z<=20)
	{
	for(int j=1;j<=z;j++)
	{
		if(s[j-1]=='z')
		{
		x=x+1;
		}
		else if(s[j-1]=='o')
		{
			y=y+1;
		}
	}
	}
	
	if(y==2*x)
	{
	    printf("Yes");
	}
	else 
		{
	    printf("NO");
	}
return 0;
	
}
