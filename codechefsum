#include<stdio.h>
int main()
{
    int t=0;
    int a=0;int b=0;
    scanf("%d",&t);
    int sum[t];
    for(int i=1;i<=t;i++)
    {
        scanf("%d\t",&a);
        scanf("%d",&b);
        sum[i-1]=a-b;
        
        
    }
    for(int j=1;j<=t;j++)
    {
        printf("%d\n",&sum[j-1]);
    }
    return 0;
}
